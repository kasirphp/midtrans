{
    "name": "kasir/midtrans",
    "description": "A framework agnostic Midtrans client PHP package.",
    "authors": [
        {
            "name": "Risang Baskoro",
            "email": "risangbaskoro@gmail.com"
        }
    ],
    "support": {
        "email": "contact@kasirphp.com",
        "issues": "https://github.com/kasirphp/midtrans/issues",
        "source": "https://github.com/kasirphp/midtrans"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/risangbaskoro"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "php-http/discovery": "^1.19"
    },
    "require-dev": {
        "rector/rector": "^0.18",
        "friendsofphp/php-cs-fixer": "^3.38",
        "pestphp/pest": "^2.24",
        "symfony/var-dumper": "^6.3",
        "guzzlehttp/guzzle": "^7.8",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Kasir\\Midtrans\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kasir\\Midtrans\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
